    "react-native-dotenv": "^3.4.8",
    "react-test-renderer": "18.1.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|react-native-vector-icons)/)",
      "node_modules/(?!axios)/"
